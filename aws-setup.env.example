# Environment Variables for AWS Deployment
# Copy this file to .env and fill in your values

# Database Configuration (AWS RDS)
DATABASE_URL=postgresql://username:password@your-rds-endpoint:5432/girlgram

# Session Security
SESSION_SECRET=your-very-secure-random-string-at-least-32-characters

# Replit Auth Configuration (keep these for OAuth)
REPL_ID=your-repl-id
REPLIT_DOMAINS=your-lambda-api-gateway-domain.com
ISSUER_URL=https://replit.com/oidc

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key